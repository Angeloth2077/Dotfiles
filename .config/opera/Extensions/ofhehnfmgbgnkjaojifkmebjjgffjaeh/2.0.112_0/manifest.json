   "content_scripts": [ {
      "exclude_matches": [ "*://www.youtube.com/embed/*", "*://www.youtube.com/pop-up-player/*" ],
      "js": [ "content.js" ],
      "matches": [ "*://www.youtube.com/*" ],
      "run_at": "document_end"
   }, {
      "exclude_matches": [ "*://www.youtube.com/embed/*", "*://www.youtube.com/pop-up-player/*", "*://www.youtube.com/live_chat/*" ],
      "js": [ "start.js" ],
      "matches": [ "*://www.youtube.com/*" ],
      "run_at": "document_start"
   }, {
      "exclude_matches": [ "*://www.youtube.com/pop-up-player/*", "*://www.youtube.com/live_chat/*" ],
      "js": [ "idle.js" ],
      "matches": [ "*://www.youtube.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "embed.js" ],
      "matches": [ "*://www.youtube.com/embed/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "pop-up-player.css" ],
      "js": [ "pop-up-player.js" ],
      "matches": [ "*://www.youtube.com/pop-up-player/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "live-chat.js" ],
      "matches": [ "*://www.youtube.com/live_chat*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en_US",
   "description": "__MSG_description__",
   "developer": {
      "name": "Maxime RF",
      "url": "https://www.mrfdev.com/enhancer-for-youtube"
   },
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Enhancer for YouTube",
   "options_page": "options.html",
   "permissions": [ "cookies", "storage", "*://www.youtube.com/*" ],
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "2.0.112",
   "web_accessible_resources": [ "resources/*" ]
}
